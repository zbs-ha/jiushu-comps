{
  "name": "jiushu-comps",
  "version": "0.3.8",
  "private": true,
  "scripts": {
    "build": "npm run tailwind && vue-cli-service build --modern",
    "serve": "vue-cli-service serve",
    "mock": "node ./mock/server/mock-server.js",
    "tailwind": "tailwindcss -i ./src/styles/tailwind/input.css -o ./src/styles/tailwind/output.css",
    "tailwind:watch": "tailwindcss -i ./src/styles/tailwind/input.css -o ./src/styles/tailwind/output.css --watch",
    "build:comps": "vue-cli-service build --target lib --formats umd-min --report libs/index.ts && npm pack && shx mv ./*.tgz ./packages/",
    "serve:tailwind": "npm run serve & npm run tailwind:watch"
  },
  "files": [
    "dist/*"
  ],
  "main": "./dist/jiushu-comps.umd.min.js",
  "dependencies": {
    "ant-design-vue": "^1.7.8",
    "core-js": "^3.6.5",
    "echarts": "^5.3.2",
    "jiushu-comps": "file:packages/jiushu-comps-0.3.8.tgz",
    "lodash": "^4.17.21",
    "uuid": "^8.3.2",
    "vue": "^2.6.11",
    "vue-echarts": "^6.0.2",
    "vue-grid-layout": "^2.3.12",
    "vue-router": "^3.2.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.17",
    "@vue/cli-plugin-eslint": "~4.5.17",
    "@vue/cli-plugin-router": "~4.5.17",
    "@vue/cli-plugin-typescript": "~4.5.17",
    "@vue/cli-service": "~4.5.17",
    "@vue/composition-api": "^1.5.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-plugin-import": "^1.13.5",
    "body-parser": "^1.20.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "dotenv": "^16.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-vuejs-accessibility": "^1.1.0",
    "express": "^4.17.3",
    "mockjs": "^1.1.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "shx": "^0.3.4",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.4.4",
    "vue-template-compiler": "^2.6.11"
  }
}
